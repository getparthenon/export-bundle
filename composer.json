{
    "name": "parthenon/export-bundle",
    "description": "An utility bundle for Symfony to help with data exports",
    "type": "library",
    "license": "mit",
    "autoload": {
        "psr-4": {
            "Parthenon\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Iain Cambridge",
            "email": "iain@humblyarrogant.io"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.1",
        "symfony/messenger": "^6.1",
        "symfony/security-bundle": "^6.1",
        "symfony/security-core": "^6.1"
    },
    "require-dev": {
        "doctrine/dbal": "^3.4",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/mongodb-odm-bundle": "^4.5",
        "doctrine/orm": "^2.13",
        "docraptor/docraptor": "^3",
        "mpdf/mpdf": "^8.1",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "3.12.0",
        "league/flysystem-async-aws-s3": "^2.5",
        "league/flysystem-aws-s3-v3": "^2.5",
        "dg/bypass-finals": "~1.3.0",
        "elasticsearch/elasticsearch": "^7.17",
        "symfony/form": "^6.1",
        "monolog/monolog": "^2.8",
        "mailgun/mailgun-php": "^3.5",
        "knplabs/knp-snappy": "^1.4",
        "wildbit/postmark-php": "^4.0",
        "symfony/mailer": "^6.1",
        "sendgrid/sendgrid": "~7.11",
        "chadhutchins/oauth2-slack": "^1.2.2",
        "nyholm/psr7": "^1.5",
        "symfony/twig-bridge": "^6.1",
        "symfony/phpunit-bridge": "^6.1"
    },
    "suggest": {

        "knplabs/knp-snappy": "^1.4",
        "docraptor/docraptor": "^3",
        "mpdf/mpdf": "^8.1"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.*"
        }
    }
}
